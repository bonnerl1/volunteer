.rank{
  width: 90%;
  /* background-color: pink; */
  margin: 0 auto;
}
.title image{
  width: 50rpx;
  height: 50rpx;
  float: left;
  margin-right: 15rpx;
}
.text{
  float: left;
  font-size: 30rpx;
  font-weight: 700;
}
.title{
  /* background-color: pink; */
  /* background-color: gray; */
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  margin: 20rpx 0;
}
.user{
  width: 90%;
  background-color: #fff;
  border-radius: 20rpx;
  overflow: hidden;
  padding: 0 30rpx;
  box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.2);
}
.content{
  width: 100%;
  height: 120rpx;
  background-color: #fff;
  line-height: 120rpx;
  padding: 15rpx 0;
  border-bottom: 0.5rpx solid rgb(218, 218, 218);
  border-top: 0.5rpx solid rgb(199, 199, 199);
}
.content image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  float: left;
  margin-top: 10rpx;
}
.num {
  float: left;
  margin-right: 15rpx;
  font-size: 30rpx;
}
.name {
  float: left;
  margin: 0 20rpx;
  font-size: 28rpx;
}
.integral{
  float: right;
  font-size: 28rpx;
}