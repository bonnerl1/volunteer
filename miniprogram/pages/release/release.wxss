.title{
  width: 200rpx;
  height: 70rpx;
  text-align: center;
  margin-left: 5%;
  margin-top: 20rpx;
  background-color: #f06469;
  border-radius: 20rpx; 
  line-height: 60rpx;
  color: #fff;
}
.prize{
  width: 42%;
  /* height: 500rpx; */
  border-radius: 20rpx;
  float: left;
  margin-left: 5%;
  margin-top: 5%;
  background-color: rgb(224, 208, 208);
  box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.2);
}
.prize image{
  width: 100%;
  height: 300rpx;
  border-radius: 20rpx;
}
.prize-text{
  width: 100%;
  text-align: center;
  font-weight: 700;
  font-size: 36rpx;
  margin-bottom: 7rpx;
  height: 45rpx;
  color: rgb(49, 49, 49);
}
.integral{
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: gray;
}
#btn{
  width: 230rpx;
  height: 60rpx;
  font-size: 30rpx;
  padding: 0;
  margin: 20rpx auto;
  line-height: 60rpx;
  color: #fff;
  background-color: #f04f54;
}